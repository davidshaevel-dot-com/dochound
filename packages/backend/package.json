{
  "name": "@dochound/backend",
  "version": "0.1.0",
  "description": "DocHound backend - Multi-tenant RAG document intelligence",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsc",
    "index": "tsx src/scripts/index-tenant.ts",
    "index:all": "tsx src/scripts/index-tenant.ts --all",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "verify:tenants": "tsx src/scripts/verify-tenants.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "llamaindex": "^0.8.26",
    "openai": "^4.77.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.5",
    "eslint": "^9.39.2",
    "globals": "^17.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.54.0"
  }
}
